<table class = "table table-striped">
  <tbody>
    <h3>Hi, <%= User.find_by_id(current_user.id).email%><h3>
    <div class="panel-group">
      <% @hws.each do |_, value| %>
      <tr>
          <div class="panel panel-default">
            <div class="panel-heading">
              <h4 class="panel-title">
                <a data-toggle="collapse" href="#collapse<%= value[0].name %>"><%= value[0].name%></a>
              </h4>
            </div>
            <div id="collapse<%= value[0].name %>" class="panel-collapse collapse">
              <ul class="list-group">
                <li class="list-group-item"><%= value[0].due_date %></li>
                <li class="list-group-item"><%= value[2] %></li>
                <li class="list-group-item"><%= value[1].comment %></li>
              </ul>
              <div class="panel-footer">Footer</div>
            </div>
          </div>
      </tr>
    <% end %>
  </div>
  <tbody>
<table>
